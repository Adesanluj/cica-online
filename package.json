{
    "name": "cica-online",
    "private": true,
    "version": "0.1.0",
    "description": "Express test",
    "keywords": [],
    "dependencies": {
        "ajv": "^6.5.2",
        "ajv-errors": "^1.0.0",
        "body-parser": "^1.13.3",
        "compression": "^1.5.2",
        "cookie-parser": "^1.4.3",
        "exegesis-express": "^1.0.0",
        "explore": "latest",
        "express": "^4.16.3",
        "glob": "^6.0.4",
        "govuk-frontend": "^1.3.0",
        "json-schema-to-x": "../json-schema-to-x",
        "method-override": "^2.3.0",
        "morgan": "^1.9.0",
        "node-sass": "^4.9.3",
        "nunjucks": "^3.1.3",
        "q-api": "../q-api",
        "q-router": "file:../q-router",
        "questionnaire-service": "file:../questionnaire",
        "redoc": "^2.0.0-alpha.30",
        "serve-favicon": "^2.3.0",
        "speccy": "^0.7.3",
        "swagger-ui-express": "^3.0.10",
        "yamljs": "^0.3.0"
    },
    "devDependencies": {
        "fs": "0.0.1-security",
        "jest": "^23.4.1",
        "eslint": "^4.19.1",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-config-xo": "^0.20.1",
        "eslint-plugin-import": "^2.13.0",
        "eslint-plugin-prettier": "^2.6.0",
        "husky": "^1.0.0-rc.9",
        "lint-staged": "^7.2.0",
        "nsp": "^3.2.1",
        "prettier": "^1.11.1"
    },
    "lint-staged": {
        "*.js": [
            "eslint --fix --color",
            "git add"
        ],
        "*.json": [
            "prettier --write",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": [
            "airbnb-base",
            "prettier"
        ],
        "env": {
            "jest": true,
            "node": true
        },
        "rules": {
            "prettier/prettier": [
                "error"
            ]
        },
        "plugins": [
            "prettier"
        ]
    },
    "jest": {
        "testEnvironment": "node"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "scripts": {
        "start": "node app.js && npm explore json-schema-to-x -- npm run copy",
        "test": "jest",
        "speccy": "cd schema && speccy lint schema.json -j --rules=../../../src/config/speccy/rules/default && speccy resolve schema.json -j -o openapi.yaml && yaml2json openapi.yaml -spi 4",
        "speccy-no-lint": "cd schema && speccy resolve schema.json -j -o openapi.yaml && yaml2json openapi.yaml -spi 4",
        "sass": "node-sass node_modules/govuk-frontend/all.scss public/assets/css/styles.css",
        "sass-ie8": "node-sass node_modules/govuk-frontend/all-ie8.scss public/assets/css/styles-ie8.css",
        "copy-backup": "copyfiles -u 2 node_modules/govuk-frontend/components/**/*.njk src/views/ && copyfiles -u 3 node_modules/govuk-frontend/assets/**/*.* public/assets/",
        "copy": "npm explore json-schema-to-x -- npm run copy && copyfiles -u 3 node_modules/govuk-frontend/assets/**/*.* public/assets/"
    },
    "main": "app.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/anonymous node/sails user/cica-online.git"
    },
    "author": "CICA",
    "license": "",
    "engines": {
        "node": ">=8.11"
    }
}
